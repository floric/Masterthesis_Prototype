{
  "name": "masterthesis",
  "private": true,
  "license": "MIT",
  "version": "1.0.0",
  "engines": {
    "node": ">= 6.11.1",
    "yarn": ">= 1.0.0"
  },
  "scripts": {
    "build": "lerna run build --stream",
    "start": "lerna run start --stream",
    "test": "lerna run test --stream",
    "lint": "lerna run lint --stream",
    "clean": "lerna run clean --stream && rm -rf node_modules && rm -rf data",
    "mongodb": "sudo docker run -p 127.0.0.1:27017:27017 mongo:latest",
    "deploy": "aws cloudformation update-stack --stack-name timeseries-explorer --template-body file://./deployment/cf.yml --parameters file://./.secrets.json --capabilities CAPABILITY_IAM"
  },
  "author": {
    "name": "Florian Richter"
  },
  "workspaces": [
    "packages/*"
  ],
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "lerna": "^3.1.4",
    "tslint": "^5.2.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.3"
  }
}
