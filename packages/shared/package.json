{
  "name": "@masterthesis/shared",
  "version": "1.0.0",
  "license": "MIT",
  "main": "lib/index.js",
  "dependencies": {
    "date-fns": "^1.29.0",
    "moment": "^2.22.1",
    "sleep-promise": "^6.0.0",
    "uuid": "^3.2.1"
  },
  "typings": "lib/index.d.ts",
  "author": {
    "email": "floririchte@gmail.com",
    "name": "Florian Richter"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "tslint -p . --fix",
    "clean": "rm -rf dist node_modules .cache",
    "test": "jest --coverage --detectLeaks"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/uuid": "^3.4.3",
    "jest": "^23.0.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.2.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.8.3",
    "weak": "^1.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/lib/**"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
